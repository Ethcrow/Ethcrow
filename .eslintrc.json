{
    "extends": "google",
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error"
    },
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint"
}
